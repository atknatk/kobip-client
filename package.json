{
  "name": "kobip",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "rimraf dist && ng build --prod  --aot=false --deploy-url \"/kobip/\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract:enum": "ngx-translate-extract -i ./src/translation -o ./src/assets/i18n/enum/kobip-tr.json -o ./src/assets/i18n/enum/kobip-en.json -c -s -m enums -f namespaced-json",
    "extract:app": "ngx-translate-extract -i ./src/app -o ./src/assets/i18n/kobip-tr.json -o ./src/assets/i18n/kobip-en.json -c -s -m _ -f namespaced-json",
    "extract": "npm run extract:app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.5",
    "@angular/common": "5.0.5",
    "@angular/compiler": "5.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "5.0.5",
    "@angular/http": "5.0.5",
    "@angular/platform-browser": "5.0.5",
    "@angular/platform-browser-dynamic": "5.0.5",
    "@angular/platform-server": "5.0.5",
    "@angular/router": "5.0.5",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "angular2-moment": "^1.7.0",
    "animate.css": "3.1.1",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.7.1",
    "codemirror": "^5.32.0",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "font-awesome": "^4.7.0",
    "hashids": "^1.1.2",
    "inputmask": "^3.3.11",
    "jquery": "^3.1.0",
    "metismenu": "^2.5.0",
    "ngx-bootstrap": "^1.6.6",
    "ngx-infinite-scroll": "^0.6.1",
    "ngx-pagination": "^3.0.3",
    "rxjs": "5.5.2",
    "signalr": "^2.2.2",
    "summernote": "0.8.8",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.0",
    "@angular/compiler-cli": "5.0.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "@types/jasmine": "2.5.38",
    "@types/jquery": "^2.0.47",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "2.4.2"
  }
}
